{
  "name": "clientapp",
  "displayName": "clientapp",
  "expo": {
    "name": "clientapp",
    "slug": "clientapp",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/path/to/myNotificationIcon.png",
          "color": "#ffffff",
          "sounds": ["./local/path/to/mySound.wav", "./local/path/to/myOtherSound.wav"],
          "mode": "production"
        }
      ]
    ],
    "android": {
      "package": "com.xinei.clientapp"
    }
  }
}
